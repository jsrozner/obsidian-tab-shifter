{
	"name": "obsidian-sample-plugin",
	"version": "1.0.1",
	"description": "Enables shifting tabs between different tab splits and some other basic IDE tab functionalities",
	"main": "main.js",
	"scripts": {
		"_prod": "NODE_ENV=production node esbuild.config.mjs production",
		"build_prod": "yarn lint && tsc -noEmit -skipLibCheck && yarn _prod",
		"dev": "NODE_ENV=development node esbuild.config.mjs",
		"dev_copy_files_for_hotload": "node ./src/scripts/copyFiles.js",
		"watch": "nodemon",
		"lint": "eslint src/*.ts",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "Joshua Rozner",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"dotenv": "^16.4.5",
		"esbuild": "0.17.3",
		"eslint": "^8.57.0",
		"nodemon": "^3.1.0",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"nodemonConfig": {
		"watch": [
			"src/",
			".env",
			"esbuild.config.mjs",
			"package.json"
		],
		"ext": "ts,js",
		"ignore": [
			"src/**/*.test.ts"
		],
		"exec": "yarn dev && yarn dev_copy_files_for_hotload"
	}
}
